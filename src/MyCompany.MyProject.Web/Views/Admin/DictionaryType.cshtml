@{
    ViewBag.ActiveMenu = PageNames.Administration.Dictionary;
    ViewBag.JsPath = "app/admin/dictionaryType";
}
<form class="layui-form layui-form-pane">
  <div class="layui-form-item">
    <div class="layui-inline">
      <div class="layui-input-inline">
        <button class="layui-btn layui-btn-normal" id="btn-add">添加</button>
        <a class="layui-btn layui-btn-primary" href="/Admin/Dictionary">字典</a>
      </div>
    </div>
  </div>
</form>
<table id="main-table" lay-filter="main"></table>
<script type="text/html" id="actionTpl">
    <button class="layui-btn layui-btn-normal layui-btn-xs" lay-event="edit">编辑</button>
    <button class="layui-btn layui-btn-danger layui-btn-xs" lay-event="delete">删除</button>
</script>