@model IReadOnlyList<MyCompany.MyProject.Dictionarys.Dto.DictionaryTypeDto>
@{
    ViewBag.ActiveMenu = PageNames.Administration.Dictionary;
    ViewBag.JsPath = "app/admin/dictionary";
}
<form class="layui-form layui-form-pane">
  <div class="layui-form-item">
    <div class="layui-inline">
      <label class="layui-form-label">类型</label>
      <div class="layui-input-inline">
          <select class="layui-select" lay-filter="selectTypeChange" lay-search id="select-type">
              @foreach (var item in Model)
              {
                  <option value="@item.Id">@item.DisplayName</option>
              }
          </select>
      </div>
    </div>
    <div class="layui-inline">
      <button class="layui-btn layui-btn-normal" id="btn-add">添加</button>
      <a class="layui-btn layui-btn-primary" href="/Admin/DictionaryType">字典类型</a>
    </div>
  </div>
</form>
<table id="main-table" lay-filter="main"></table>
<script type="text/html" id="actionTpl">
  <button class="layui-btn layui-btn-normal layui-btn-xs" lay-event="edit">编辑</button>
  <button class="layui-btn layui-btn-danger layui-btn-xs" lay-event="delete">删除</button>
</script>

